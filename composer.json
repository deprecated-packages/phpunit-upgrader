{
    "name": "symplify/phpunit-upgrader",
    "description": "Upgrade PHPUnit tests with smart helping hand",
    "license": "MIT",
    "bin": [
        "bin/phpunit-upgrader"
    ],
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "nette/robot-loader": "^3.2",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/symplify-kernel": "^9.0.25",
        "symplify/console-color-diff": "^9.0.25",
        "symplify/package-builder": "^9.0.25",
        "symplify/smart-file-system": "^9.0.25"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPUnitUpgrader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPUnitUpgrader\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
